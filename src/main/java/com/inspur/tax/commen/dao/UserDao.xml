<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserDao">
    <select id="queryUserIdByApp_key" resultType="Map" parameterType="Map">
      SELECT USER_ID FROM qa_users WHERE status = '1' and APP_KEY = #{app_key, jdbcType=VARCHAR}
   </select>
</mapper>